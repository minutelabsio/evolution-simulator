    "moment": "^2.22.2",
    "moment-duration-format": "^2.2.2",
    "nosleep.js": "^0.9.0",
    "three": "^0.111.0",
    "three-orbit-controls": "^82.1.0",
    "vue": "^2.5.17",
    "vue-analytics": "^5.17.2",
    "vue-chartjs": "^3.4.0",
    "vue-circle-slider": "^1.0.2",
    "vue-router": "^3.0.2",
    "vue-scrollto": "^2.17.1",
    "vue-slider-component": "^3.0.38",
