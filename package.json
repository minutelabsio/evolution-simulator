    "vue-template-compiler": "^2.5.17",
    "wasm-opt": "^1.1.1"
  },
  "resolutions": {
    "comlink-loader": "https://github.com/wellcaffeinated/comlink-loader#dist",
    "webpack": "4.29.6",
    "worker-loader": "https://github.com/wellcaffeinated/worker-loader"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "ignorePatterns": [
      "src/wasm/**/*.js"
    ],
    "rules": {
      "generator-star-spacing": "off",
      "comma-style": [
        2,
        "first"
      ],
      "space-in-parens": 0,
      "space-before-blocks": 0,
      "padded-blocks": 0,
      "space-before-function-paren": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
